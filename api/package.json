{
  "name": "api",
  "version": "1.0.0",
  "description": "api for bros chat",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn watch:build\" \"yarn watch:server\"",
    "watch:build": "cross-env NODE_ENV=development webpack --watch",
    "watch:server": "cross-env NODE_ENV=development nodemon ./build/index.js --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode=production"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.0",
    "nodemon": "^2.0.16",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
